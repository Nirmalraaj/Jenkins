node{
   stage('SCM Checkout'){
     git 'https://github.com/Nirmalraaj/Jenkins/'
       echo "${currentBuild.number}"
    
     }
   stage('Deploy to airflow'){
    sshagent(['jenkins_tom']) {
    sh 'scp -o StrictHostKeyChecking=no /var/lib/jenkins/workspace/webhooks/*.py  ec2-user@54.156.115.234:/home/ec2-user/airflow/dags/'
    }
    }
    stage('copy artifacts')
        
    {  sh'pwd' 
    dir('/var/lib/jenkins/jobs/webhooks/builds/'){
       echo "${currentBuild.number}"
       sh "cd ${currentBuild.number}"
       sh 'ls'
       sh 'pwd'
        //archiveArtifacts artifacts: 'test.py', onlyIfSuccessful: true
       
      
        
        }
        
       
    }
   
}  
